{
    "manifestVersion": 1,
    "id": "devops-snapshot",
    "publisher": "FabienCamous",
    "version": "1.0.21",
    "name": "Workitem Snapshot",
    "description": "Spot project evolution between two dates at a glance",
    "public": false,
    "categories": [
        "Azure Boards"
    ],
    "scopes": [
        "vso.work"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "snapshot-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Snapshot",
                "uri": "snapshot.html"
            }
        }
    ],
    "files": [
        {
            "path": "snapshot.html",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "node_modules/@types",
            "addressable": true,
            "packagePath": "node_modules/@types"
        },
        {
            "path": "node_modules/monaco-editor",
            "addressable": true,
            "packagePath": "node_modules/monaco-editor"
        }
    ]
}